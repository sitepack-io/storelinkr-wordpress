{
    "name": "storelinkr/tests",
    "description": "Unit tests for StoreLinkr plugin",
    "type": "project",
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "StoreLinkr\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StoreLinkr\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-text"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}